{
    "manifest_version": 2,
    "name": "Social Media Launcher",
    "description": "Quick social media launcher.",
    "version": "1.0",
    "icons": {
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"],
    "content_security_policy": "script-src 'self' 'sha256-WOdSzz11/3cpqOdrm89LBL2UPwEU9EhbDtMy2OciEhs=' https://www.iana.org https://ajax.googleapis.com; object-src 'self'"

}